fun sumar(x: int, y: int): int
    return x + y
end

fun multiplicar(a: int, b: int): int
    return a * b
end

fun obtenerValor(): int
    return 42
end

fun main(): int
    resultado: int
    temp: int
    
    
    resultado = sumar(10, 20)
    
    
    resultado = sumar(sumar(1, 2), sumar(3, 4))
    
    
    temp = multiplicar(2, 3)
    resultado = sumar(temp, obtenerValor())
    
    
    resultado = sumar(multiplicar(2, 3), multiplicar(4, 5))
    
    return resultado
end
